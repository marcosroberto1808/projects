<h1>Listing documentos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo documento</th>
      <th>Assistido</th>
      <th>Inquerito policial</th>
      <th>Processo judicial</th>
      <th>Data</th>
      <th>Nome url</th>
      <th>Resposta necessidade</th>
      <th>Resposta</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @documentos.each do |documento| %>
      <tr>
        <td><%= documento.tipo_documento_id %></td>
        <td><%= documento.assistido_id %></td>
        <td><%= documento.inquerito_policial_id %></td>
        <td><%= documento.processo_judicial_id %></td>
        <td><%= documento.data %></td>
        <td><%= documento.nome_url %></td>
        <td><%= documento.resposta_necessidade %></td>
        <td><%= documento.resposta %></td>
        <td><%= link_to 'Show', documento %></td>
        <td><%= link_to 'Edit', edit_documento_path(documento) %></td>
        <td><%= link_to 'Destroy', documento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Documento', new_documento_path %>
