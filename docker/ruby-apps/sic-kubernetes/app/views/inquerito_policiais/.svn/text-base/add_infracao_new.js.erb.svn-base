<% unless @infracao["descricao_codigo"].nil? or @infracao["descricao_codigo"].empty? %>
	$("#infracoes").append('<span class="label label-danger" title="<%= @infracao.descricao_nome %>"><%= @infracao.descricao_codigo %></span>');
	$("#infracoes").append('<%= hidden_field_tag :infracao, (@infracao["descricao_codigo"].to_s+"|"+@infracao["descricao_nome"].to_s), name: "infracoes[]" %>');
<% else %>
	$("#erro_infracoes").html('Erro');
<% end %>
