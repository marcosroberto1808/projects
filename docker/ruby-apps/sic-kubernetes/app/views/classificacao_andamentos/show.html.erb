<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">Classificação de Andamento</h3>
      </div>
    <div class="panel-body">
        <div class="col-md-6">
            <p>
              <strong>Descrição:</strong>
              <%= @classificacao_andamento.descricao %>
            </p>

            <%= link_to 'Editar', edit_classificacao_andamento_path(@classificacao_andamento), :class=> "btn btn-primary" %> |
            <%= link_to 'Voltar', classificacao_andamentos_path, :class=> "btn btn-primary" %>
          </div>
      </div>
</div>
