<!doctype html>
<html>
<head>
  <meta charset='utf-8' />
  <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
  <%= wicked_pdf_stylesheet_link_tag "pdf" -%>
  <%= wicked_pdf_javascript_include_tag "number_pages" %>
  <link href="<%= "#{Rails.root}/app/assets/stylesheets/sic/relatorio-forca-tarefa.css"%>" media="screen" rel="stylesheet" />
</head>
<body onload=''>
<div id="content">
  <div>
    <h3 align="center">Relatório Quantitativo da Força Tarefa</h3>
    <div class="ficha-title">Período : <%= @data_inicial %> à <%= @data_final %> Força Tarefa : <%= @forca_tarefa_nome.nome rescue '' %></div>

    <%= render 'andamentos/partials/consultar/table_providencia' %>
    <%= render 'andamentos/partials/consultar/table_tipo_andamento' %>
    <%= render 'andamentos/partials/consultar/table_problema_identificado' %>
  </div>
</div>
</body>
</html>
