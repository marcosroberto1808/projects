<div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">Lista de Origens de Inquérito</h3>
  </div>
  <div class="panel-body">
    <div class="col-md-12">
      <% if @origem_inquerito.present? and @origem_inquerito.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(@origem_inquerito.errors.count, "erro(s)") %> impediram esta Origem Inquérito de ser salva:</h2>

        <ul>
        <% @origem_inquerito.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    <% end %>
      <%= link_to 'Cadastrar Origem de Inquérito', new_origem_inquerito_path, :class=> "btn btn-primary" %>
      <table class="table table-hover margin-top">
        <thead>
          <tr>
            <th>UF</th>
            <th>Cidade</th>
            <th>Nome</th>
            <th></th>
            <th></th>
            <th></th>
          </tr>
        </thead>

        <tbody>
          <% @origem_inqueritos.each do |origem_inquerito| %>
            <tr>
              <td><%= origem_inquerito.uf %></td>
              <td><%= origem_inquerito.cidade.nome %></td>
              <td><%= origem_inquerito.nome %></td>
               <td class="icon-width"><%= link_to 'Exibir', origem_inquerito, :class=> "intra-icon icon-exibir" %></td>
              <td class="icon-width"><%= link_to 'Editar', edit_origem_inquerito_path(origem_inquerito), :class=> "intra-icon icon-edit" %></td>
              <td class="icon-width"><%= link_to 'Deletar', origem_inquerito, :class=> "intra-icon icon-excluir", method: :delete, data: { confirm: 'Tem certeza?' } %></td>
             
            </tr>
          <% end %>
        </tbody>
      </table>

      </div>
    </div>
  </div>

