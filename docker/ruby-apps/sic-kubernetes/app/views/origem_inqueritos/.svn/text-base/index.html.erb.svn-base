<% if @origem_inquerito.present? and @origem_inquerito.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@origem_inquerito.errors.count, "erro(s)") %> impediram esta Origem Inquérito de ser salva:</h2>

    <ul>
    <% @origem_inquerito.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>

<h1>Lista de Origens de Inquérito</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>UF</th>
      <th>Cidade</th>
      <th>Nome</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @origem_inqueritos.each do |origem_inquerito| %>
      <tr>
        <td><%= origem_inquerito.uf %></td>
        <td><%= origem_inquerito.cidade.nome %></td>
        <td><%= origem_inquerito.nome %></td>
        <td><%= link_to 'Exibir', origem_inquerito %></td>
        <td><%= link_to 'Editar', edit_origem_inquerito_path(origem_inquerito) %></td>
        <td><%= link_to 'Deletar', origem_inquerito, method: :delete, data: { confirm: 'Tem Certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Cadastrar Origem de Inquerito', new_origem_inquerito_path %>
